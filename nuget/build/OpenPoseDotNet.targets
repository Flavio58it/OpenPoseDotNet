<Project>
  <PropertyGroup>
    <NativeDependenciesVersion>1.4.0.20190115</NativeDependenciesVersion>
  </PropertyGroup>

  <PropertyGroup Condition="$(NugetPackageRoot)==''">
    <NugetPackageRoot>
      "$([MSBuild]::GetPathOfFileAbove($(MSBuildThisFile), $(MSBuildThisFileDirectory)..\..\))"
    </NugetPackageRoot>
  </PropertyGroup>

 <Target Name="EmitNugetPackageRootMessage">
   <Message Importance="High" Text='$(NugetPackageRoot)' />
 </Target>
</Project>